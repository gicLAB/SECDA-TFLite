{ 
  "name": "secda-tflite",
  "build": {
    "dockerfile": "Dockerfile",
    "context": ".."
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode.cmake-tools",
        "ms-vscode.cpptools",
      ]
    }
  },
  "mounts": [
    "source=${localWorkspaceFolder},target=/working_dir,type=bind,consistency=cached",
    "source=${localEnv:vivado_2019_path},target=/working_dir/Vivado_2019,type=bind,consistency=cached",
    "source=${localEnv:vivado_2024_path},target=/working_dir/Vivado_2024,type=bind,consistency=cached"


  ],
  "postCreateCommand": "conda run -n secda_tflitev2 pip install -r requirements.txt"


}